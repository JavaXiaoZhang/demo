<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox spacing="50.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button alignment="TOP_LEFT" text="校验文件" onAction="#onCompareButtonClick"/>

    <VBox alignment="CENTER" spacing="20.0">
        <Button text="上传上期文件夹" onAction="#onPreviousButtonClick"/>
        <Button text="上传当期文件夹" onAction="#onCurrentButtonClick"/>
        <Button text="开始校验" onAction="#onStartButtonClick"/>
    </VBox>
</VBox>
